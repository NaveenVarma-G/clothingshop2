@model ClothingStore.DataAccess.ProductDetail

@{
    ViewData["Title"] = "Details";
}
<link rel="stylesheet" href="~/css/productinventory.css" asp-append-version="true" />
<h1>Product Details</h1>

<div>
    
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.ProductName)
        </dt>
